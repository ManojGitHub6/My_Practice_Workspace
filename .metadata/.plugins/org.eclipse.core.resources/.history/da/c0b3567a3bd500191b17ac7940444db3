package testng;

import org.testng.annotations.Test;

public class Grouping {
	
	
	@Test(groups= {"perform"})
	public void test1() {
		
		System.out.println("first");
	}
	
	@Test
	public void test2() {
		
		System.out.println("second");
	}
	
	@Test
	public void test3() {
		
		System.out.println("third");
	}

}
